  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/",
      "args": [
        "-lc",
        "./src/config/logging_config.json",
        "-pc",
        "./src/config/plate_extraction_config.json",
        "-ec",
        "./src/config/lpe_config.json",
        "-dp",
        "./data/Vehicles",
        "-r",
        "1",
        "100",
        "-o",
        "./data/output",
        "-d"
      ]
    }
  ]
}
